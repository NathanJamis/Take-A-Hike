    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcrypt-nodejs": "0.0.3",
    "connect-mongodb-session": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.9",
